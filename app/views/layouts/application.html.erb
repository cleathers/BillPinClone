<!DOCTYPE html>
<html>
<head>
  <title>BillPinClone</title>
  <link rel='stylesheet' href='https://app.billpin.com/static/css/billpin_desktop.css?rev=16'>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => false %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => false %>
  <%= csrf_meta_tags %>
</head>
<body   >

<div class='wrapper'>
<% if current_page?(root_url)  && !user_signed_in? %>
  <%= render partial: 'layouts/parallax' %>
<% end %>
</div>

<%= render partial: 'layouts/navbar' %>
<%= render partial: 'layouts/flash_messages' %>

<div class='container'>
  <%= yield %>
</div>

<%= render partial: 'helpers/parallax' %>
</body>
</html>
<% if current_page?(root_url) %>
  <script src='/parallax.js'></script>
<% end %>
